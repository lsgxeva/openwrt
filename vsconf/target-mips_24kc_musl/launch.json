{
    "configurations": [
        {
            "name": "(gdb) openwrt Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../../build_dir/target-mips_24kc_musl/${workspaceRootFolderName}/${workspaceRootFolderName}",
            "args": [
                "--daemon",
                "--logfile", "/tmp/log/${workspaceRootFolderName}.log"
            ],
            "sourceFileMap": {},
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/src",
            "environment": [{ "name": "config", "value": "debug" }],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "${workspaceFolder}/../../../staging_dir/toolchain-mips_24kc_gcc-12.3.0_musl/bin/mips-openwrt-linux-musl-gdb",
            "miDebuggerServerAddress": "192.168.11.1:14399",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set sysroot for gdb",
                    "text": "set sysroot ${workspaceFolder}/../../../staging_dir/toolchain-mips_24kc_gcc-12.3.0_musl",
                    "ignoreFailures": true
                }
            ]
        }
    ],
    "version": "0.2.0"
}
